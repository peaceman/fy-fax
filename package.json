{
  "name": "fy-fax",
  "version": "0.0.0",
  "description": "funyard fax",
  "author": "nixalio@gmail.com",
  "bin": {
    "fy-fax": "./bin/run"
  },
  "homepage": "https://github.com/peaceman/fy-fax",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "peaceman/fy-fax",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.6",
    "@types/js-yaml": "^4.0.5",
    "bluebird": "^3.7.2",
    "csv-parse": "^5.3.1",
    "fastify": "^3.29.2",
    "google-libphonenumber": "^3.2.31",
    "http": "^0.0.1-security",
    "inquirer": "^8.2.4",
    "iter-ops": "^1.6.4",
    "js-yaml": "^4.1.0",
    "nunjucks": "^3.2.3",
    "p-queue": "^6.6.2",
    "puppeteer": "^14.4.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sqlite3": "^5.0.11",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/bluebird": "^3.5.37",
    "@types/chai": "^4",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/inquirer": "^8.2.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.3",
    "@types/nunjucks": "^3.2.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "fy-fax",
    "dirname": "fy-fax",
    "commands": "./dist/cli/commands",
    "hooks": {},
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "recipient": {
        "description": "Manage recipients."
      },
      "template": {
        "description": "Manage templates."
      },
      "run": {
        "description": "Render and send faxes."
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/peaceman/fy-fax/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
